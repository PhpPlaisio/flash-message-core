{
  "name": "plaisio/flash-message-core",
  "description": "PhpPlaisio: Core Flash Message",
  "keywords": [
    "PhpPlaisio",
    "Flash Message",
    "Core"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "plaisio-ts/helper-cast": "^1.0",
    "plaisio/flash-message": "dev-master",
    "plaisio/helper-html": "^1.2",
    "plaisio/kernel": "^3.0",
    "plaisio/session-core": "^4.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phing/phing": "dev-master",
    "phpunit/phpunit": "^9.0",
    "plaisio/console": "^2.4.4",
    "plaisio/phing-extensions": "^2.0.7",
    "setbased/phing-extensions": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\FlashMessage\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\FlashMessage\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
