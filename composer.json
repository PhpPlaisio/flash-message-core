{
  "name": "plaisio/flash-message-core",
  "description": "PhpPlaisio: Core Flash Message",
  "keywords": [
    "PhpPlaisio",
    "Flash Message",
    "Core"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "plaisio-ts/helper-cast": "^2.1.0",
    "plaisio/cgi": "^1.4.0",
    "plaisio/exception": "^1.3.0",
    "plaisio/flash-message": "^2.1.0",
    "plaisio/helper-html": "^4.2.0",
    "plaisio/kernel": "^3.3.0",
    "plaisio/page-core": "^1.6.0",
    "plaisio/request": "^1.0.1",
    "plaisio/response-core": "^2.9.0",
    "plaisio/session-core": "^5.6.0",
    "symfony/polyfill-uuid": "^1.31.0"
  },
  "require-dev": {
    "phing/phing": "^3.0.1",
    "phpunit/phpunit": "^11.5.22",
    "plaisio/console-kernel": "^1.2.0",
    "plaisio/console-type-script": "^1.2.0",
    "plaisio/phing-extensions": "^3.4.0",
    "setbased/phing-extensions": "^3.3.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\FlashMessage\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\FlashMessage\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
