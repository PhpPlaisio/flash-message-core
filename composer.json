{
  "name": "plaisio/flash-message-core",
  "description": "PhpPlaisio: Core Flash Message",
  "keywords": [
    "PhpPlaisio",
    "Flash Message",
    "Core"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "plaisio-ts/helper-cast": "^2.1.0",
    "plaisio/cgi": "^1.3.0",
    "plaisio/exception": "^1.2.0",
    "plaisio/flash-message": "2.0.0",
    "plaisio/helper-html": "^4.1.0",
    "plaisio/kernel": "^3.2.1",
    "plaisio/page-core": "^1.5.0",
    "plaisio/request": "^0.13.1",
    "plaisio/response-core": "^2.8.0",
    "plaisio/session-core": "^5.2.0",
    "symfony/polyfill-uuid": "^1.27.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phing/phing": "^3.0.0",
    "phpunit/phpunit": "^9.6.3",
    "plaisio/console-kernel": "^1.0.1",
    "plaisio/console-type-script": "^1.0.0",
    "plaisio/phing-extensions": "^3.2.0",
    "setbased/phing-extensions": "^3.1.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\FlashMessage\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\FlashMessage\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
